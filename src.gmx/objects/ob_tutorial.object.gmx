        <arguments>
          <argument>
            <kind>1</kind>
            <string>switch (textos)
{
    case 2:
        instance_create(192,128,ob_infanteria_azul);
        textos = 3;
        break;
        
    case 4:
        ob_infanteria.PV = -1;
        instance_create(352,352,ob_cohetes);
        textos = 5;
        break;
    
    case 6:
        instance_create(352,256,ob_tanque_azul);
        textos = 7;
        break;
        
    case 12:
        with (ciu_roja)
        {
            instance_change(ciu_neutral, true);
        }
        textos = 13;
        break;
    
    case 17:
        with (UNIDAD)
        {
            instance_destroy();
        }
        textos = 18;
        controlador.dineroRojo = 50;
        break;
    
    case 5: case 8: case 9: case 10: case 11: case 14: case 15: case 16: case 20: case 21: case 22:
        textos++;
        break;
        
    case 23:
        alarm[0] = 30;
        ob_fading.oscurecer = true;
        break;
        
    
}
</string>
          </argument>
        </arguments>
      </action>
